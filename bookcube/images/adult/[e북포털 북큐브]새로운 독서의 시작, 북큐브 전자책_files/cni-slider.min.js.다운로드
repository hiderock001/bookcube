$(function(){$.fn.namilSlider=function(t){t=$.extend({width:306,delay:100},t);var i=t.width,n=t.delay,e=$(this).children("li").length,s=i*e,l=0;$(this).css("width",s),$(this).find("li:last").prependTo($(this)),$(this).css("left",-i),$(".event-next").on("click",function(){var t=Math.abs(l-$.now());t>300&&(l=$.now(),$(this).prev().find("ul").stop().animate({left:parseInt($(this).prev().find("ul").filter(":not(:animated)").css("left"))-i+"px"},n,"swing",function(){$(this).children("li").first().appendTo($(this)),$(this).css("left",-i+"px")}))}),$(".event-prev").on("click",function(){var t=Math.abs(l-$.now());t>300&&(l=$.now(),$(this).next().find("ul").stop().animate({left:parseInt($(this).next().find("ul").filter(":not(:animated)").css("left"))+i+"px"},n,"swing",function(){$(this).css("left",-i+"px"),$(this).children("li").last().prependTo($(this))}))})}});